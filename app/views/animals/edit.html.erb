<h1>Edit details for <%= @animal.name %></h1>

<%= form_with model: @animal do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :species %>
  <%= f.select :species, SPECIES %>
  <%= f.label :breed %>
  <%= f.select :breed, DOG_BREEDS %>
  <%= f.label :gender %>
  <%= f.select :gender, GENDER %>
  <%= f.submit %>
<% end %>

<%= link_to "Back", animal_path(@animal) %>
